<% var wallet_js_url = process.env.NODE_ENV=='development'?'http://127.0.0.1:3001/wallet-connect/bundle.js':'/wallet-connect/bundle.js' %>
<script src="<%=wallet_js_url%>"></script>
<div class='modal fade' id='pickNFTModal' tabindex='-1'>
        <div class='modal-dialog'>
                <div class='modal-content'>
                        <div class='modal-header'>
                              Please choose one nft image for your profile.
                        </div>
                        <div class='modal-body'>
                           NFT images will be here.
                        </div>
                        <div class='modal-footer'>
                                <button type='button' class='cancel-settings btn btn-simple raydium-theme-button active' data-dismiss='modal'>Close</button>
                        </div>
                </div>
        </div>
</div>

