{
  "name": "expresssite",
  "version": "0.0.0",
  "private": true,
  "main": "./bin/www",
  "scripts": {
    "start": "node ./bin/www",
    "common-obfuscator": "javascript-obfuscator ./public/javascript --output ./build/javascript --config ./obfuscation_config.js",
    "wallet-obfuscator": "javascript-obfuscator ./build/wallet-connect --output ./build/wallet-connect --config ./obfuscation_config.js",
    "obfuscator": "npm run common-obfuscator && npm run wallet-obfuscator",
    "dev": "cross-env NODE_ENV=development nodemon --ignore public/ --ignore wallet-connect/ --ignore views/",
    "wallet-connect-start": "webpack serve --hot --host 0.0.0.0 --config=webpack.dev.config.js --mode development",
    "wallet-connect-build": "cross-env NODE_ENV=production webpack  --config=webpack.prod.config.js --mode production",
    "build": "npm run wallet-connect-build && npm run obfuscator",
    "deploy": "npm run build && gcloud app deploy --quiet"
  },
  "dependencies": {
    "@babel/core": "^7.13.10",
    "@babel/plugin-transform-runtime": "^7.13.10",
    "@babel/preset-env": "^7.13.10",
    "@babel/preset-react": "^7.13.13",
    "@babel/preset-typescript": "^7.9.0",
    "@solana/wallet-adapter-base": "^0.6.0",
    "@solana/wallet-adapter-react": "^0.12.4",
    "@solana/wallet-adapter-react-ui": "^0.5.1",
    "@solana/wallet-adapter-wallets": "^0.10.1",
    "@solana/web3.js": "^1.20.0",
    "babel": "^6.23.0",
    "babel-loader": "^8.2.2",
    "body-parser": "^1.18.3",
    "connect-mongo": "^2.0.1",
    "cookie-parser": "~1.4.3",
    "cross-env": "^7.0.3",
    "debug": "~2.6.9",
    "dotenv": "^6.0.0",
    "ejs": "~2.5.7",
    "express": "~4.16.0",
    "express-session": "^1.15.6",
    "express-socket.io-session": "^1.3.5",
    "html-webpack-plugin": "^5.4.0",
    "http-errors": "~1.6.2",
    "mongoose": "^5.6.11",
    "morgan": "^1.9.1",
    "object-hash": "^1.3.0",
    "passport": "^0.4.0",
    "passport-discord": "^0.1.4",
    "passport-oauth": "^1.0.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-hot-toast": "^2.1.1",
    "request": "^2.88.0",
    "request-promise": "^4.2.2",
    "shortid": "^2.2.13",
    "socket.io": "^2.2.0",
    "ts-loader": "^9.2.6"
  },
  "devDependencies": {
    "@types/node": "^12.0.0",
    "@types/react": "^17.0.24",
    "@types/react-dom": "^17.0.0",
    "css-loader": "^5.1.1",
    "css-modules-typescript-loader": "^4.0.1",
    "javascript-obfuscator": "^2.19.0",
    "nodemon": "^2.0.13",
    "react-scripts": "^4.0.3",
    "source-map-explorer": "^2.5.2",
    "style-loader": "^2.0.0",
    "typescript": "^4.4.3",
    "webpack": "^5.30.0",
    "webpack-cli": "^4.6.0",
    "webpack-dev-server": "^3.11.2",
    "yarn": "^1.22.11"
  }
}
